{
    "name": "kiboit/phast",
    "description": "A toolbox for optimizing web page performance",
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": ">=5.6",
        "mrclay/jsmin-php": "^2.3.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "facebook/webdriver": "^1.5",
        "ext-dom": "*",
        "nikic/php-parser": "^3.1",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Kibo\\Phast\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kibo\\Phast\\": ["test/php/"]
        }
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    }
}
